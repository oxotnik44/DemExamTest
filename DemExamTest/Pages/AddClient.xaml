<Page x:Class="DemExamTest.Pages.AddClient"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DemExamTest.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddClient">

    <Grid>
        <StackPanel>
            <TextBox Height="30" Width="500" Margin="0,10,0,10" Name="LastName"/>
            <TextBox Height="30" Width="500" Margin="0,0,0,10" Name="FirstName"/>
            <TextBox Height="30" Width="500" Margin="0,0,0,10" Name="Patronymic"/>
            <ComboBox Name="gen"/>
            <TextBox Height="30" Width="500" Margin="0,0,0,10" Name="Phone"/>
            <DatePicker Name="Birthday"/>
            <TextBox Height="30" Width="500" Margin="0,0,0,10" Name="Email"/>
            <DatePicker Name="RegistrationDate"/>
            <Label/>
            <Button Height="30" Width="100" Click="Button_Click" />
        </StackPanel>
       
    </Grid>
</Page>
