<Window x:Class="DemExamTest.Pages.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemExamTest.Pages"
        mc:Ignorable="d"
        Title="Main" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Button Height="40" Width="100" Grid.Column="0" Content="Add" Name="Add" Click="Add_Click"/>
                <Button Height="40" Width="100" Grid.Column="1" Content="Order" Name="Order" Click="Order_Click"/>
                <Button Height="40" Width="100" Grid.Column="2" Content="View" Name="View" Click="View_Click" />

            </Grid>
           
        </StackPanel>
        <Frame x:Name="frame" Navigated="frame_Navigated" Grid.Row="1"/>
    </Grid>
</Window>
