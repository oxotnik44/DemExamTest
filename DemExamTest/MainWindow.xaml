<Window x:Class="DemExamTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemExamTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Icon="./Resources/beauty_logo.png"
        >
    <Grid>
        <StackPanel>
            <TextBox x:Name="firstName" Text="Дэнна"/>
            <TextBox x:Name="lastName" Text="Кузьмина"/>
            <Button x:Name="auth" Height="40" Click="auth_Click"/>
        </StackPanel>
    </Grid>
</Window>
